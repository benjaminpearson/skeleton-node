{
  "plugins": ["plugins/markdown"],
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src", "docs/JSDOC.md"],
    "includePattern": ".js$",
    "excludePattern": ".test.js$"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "destination": "./docs/jsdoc",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/docdash"
  }
}
